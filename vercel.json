{
  "version": 2,
  "name": "talabat-pos-platform",
  "description": "Minimal Talabat POS Integration Platform for Vercel",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}